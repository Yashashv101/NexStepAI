{
  "name": "nexstep-ai",
  "version": "1.0.0",
  "description": "AI-powered career roadmap and skill gap analysis platform",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "cd client && npm run dev",
    "ai": "cd ai-ml && python main.py",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run ai\"",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "career",
    "roadmap",
    "AI",
    "skill-gap",
    "learning"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
